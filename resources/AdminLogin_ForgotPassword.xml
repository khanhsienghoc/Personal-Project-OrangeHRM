<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Admin- Forgot Password Test Suite">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
        <listener class-name="reportConfigs.MethodListener"></listener>
        <listener class-name="reportConfigs.AllureTestListener"></listener>
    </listeners>
    <test name="Run on chrome">
        <parameter name="browser" value="chrome"/>
        <parameter name="environment" value="local"/>
        <classes>
            <class name="admin.User_02_Admin_ForgotPassword"/>
        </classes>
    </test>
</suite>